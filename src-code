        "#Q1 = df.quantile(0.25)\n",
        "#Q3 = df.quantile(0.75)\n",
        "Q1,Q3 = np.percentile(df.Salary, [25,75])\n",
        "IQR = Q3 - Q1\n",
        "\n",
        "print(\"Outlier Data = \" + str(IQR))\n",
        "\n",
        "lower = Q1 - (1.5 * IQR)\n",
        "upper = Q3 + (1.5 * IQR)\n",
        "print(\"Lower bound =\" + str(lower))\n",
